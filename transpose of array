import java.util.Scanner;
public class Q4 {
    public static void main(String[] args) {
        int[][] array={{10,20,30},{40,50,60}};
        for(int i=0;i<array[0].length;i++)
        {
            for(int j=0;j<array.length;j++)
            {
                System.out.print(array[j][i]+" ");
            }
            System.out.println(" ");
        }
    }
}
